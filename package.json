{
  "name": "@nuxt/nuxt-atomic",
  "version": "0.1.0",
  "description": "Nuxt 4 module for atomic (saga-like) multi-step transactions with automatic server routes, rollbacks, and logging.",
  "type": "module",
  "license": "MIT",
  "author": "Jairo A. Medina C.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamone19/nuxt-atomic.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/nuxt-atomic/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.mjs",
      "require": "./dist/runtime/index.cjs"
    }
  },
  "main": "dist/module.cjs",
  "module": "dist/module.mjs",
  "types": "dist/module.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "docs"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "pnpm -C examples/nuxt-app dev",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "prepare": "pnpm -w build",
    "demo": "concurrently -k -n MOCK,NUXT -c blue,green \"pnpm -C examples/mock-server start\" \"pnpm -C examples/nuxt-app dev\"",
    "test:e2e": "vitest run --coverage examples/e2e"
  },
  "peerDependencies": {
    "nuxt": "^4.0.0",
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.0",
    "defu": "^6.1.4",
    "ofetch": "^1.3.4",
    "pathe": "^1.1.1",
    "h3": "^1.11.0" 
  },
  "devDependencies": {
    "unbuild": "^2.0.0",
    "typescript": "^5.6.2",
    "eslint": "^9.13.0",
    "@types/node": "^22.9.0",
    "pnpm": "^9.11.0",
    "concurrently": "^8.2.2",
    "vitest": "^2.1.1",
    "@vitest/coverage-v8": "^2.1.1"
  },
  "packageManager": "pnpm@9.11.0"
}
